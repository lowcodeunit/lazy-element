{
  "name": "@lowcodeunit/lazy-element",
  "version": "0.0.10",
  "scripts": {
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lazy-element",
    "deploy:common": "npm version patch --prefix projects/common && ng build common && npm publish ./dist/common --access public",
    "deploy:lazy-element": "npm run build:lazy-element && npm publish ./dist/lazy-element --access public",
    "demo": "npm run build:common && npm run start",
    "build": "npm run build:common && npm run build:lazy-element",
    "build:common": "ng build common",
    "build:lazy-element": "ng build lazy-element --prod --single-bundle && npm run pack",
    "pack": "npm run pack:lazy-element",
    "pack:lazy-element": "mkdirp dist/wc/lazy-element && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:join": "concat-glob-cli -f \"dist/wc/lazy-element/lazy-element.*.js\" -o dist/wc/lazy-element/lazy-element.js",
    "pack:main": "concat-glob-cli -f \"dist/lazy-element/main.*.js\" -o dist/wc/lazy-element/lazy-element.1.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lazy-element/scripts.*.js\" -o dist/wc/lazy-element/lazy-element.0.js",
    "ng": "ng",
    "start": "ng serve demo",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node projects/lazy-element/copy-wc-polyfill.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/elements": "^7.2.3",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@lowcodeunit-devkit/ng-lcu": "0.0.124",
    "@webcomponents/custom-elements": "^1.2.1",
    "@webcomponents/webcomponentsjs": "^2.2.6",
    "copy": "^0.3.2",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^7.7.5",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@lcu/cli": "0.0.86",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concat-glob-cli": "^0.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
