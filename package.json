{
  "name": "@lowcodeunit/lazy-element",
  "version": "0.0.150",
  "scripts": {
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lazy-element",
    "deploy:common": "npm version patch --prefix projects/common && ng build common && npm publish ./dist/common --access public",
    "deploy:lazy-element": "npm version patch --prefix projects/lazy-element && npm run build:lazy-element && npm publish ./dist/lazy-element --access public",
    "demo": "npm run build:common && npm run start",
    "build": "npm run build:common && npm run build:lazy-element",
    "build:common": "ng build common",
    "build:lazy-element": "ng build lazy-element --prod --single-bundle && npm run pack",
    "pack": "npm run pack:lazy-element",
    "pack:lazy-element": "mkdirp dist/wc/lazy-element && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:join": "concat-glob-cli -f \"dist/wc/lazy-element/lazy-element.*.js\" -o dist/wc/lazy-element/lazy-element.js",
    "pack:main": "concat-glob-cli -f \"dist/lazy-element/main.*.js\" -o dist/wc/lazy-element/lazy-element.1.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lazy-element/scripts.*.js\" -o dist/wc/lazy-element/lazy-element.0.js",
    "ng": "ng",
    "start": "ng serve demo",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node projects/lazy-element/copy-wc-polyfill.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/elements": "^11.1.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^11.1.2",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@aspnet/signalr": "^1.1.4",
    "@lcu/common": "^1.77.686",
    "@webcomponents/custom-elements": "^1.4.2",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "copy": "0.3.2",
    "core-js": "^3.6.5",
    "document-register-element": "^1.14.5",
    "ngx-build-plus": "^10.1.1",
    "rxjs": "~6.6.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.4",
    "@angular-devkit/build-ng-packagr": "^0.1001.0",
    "@angular/cli": "^11.1.4",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/language-service": "^11.1.2",
    "@lcu/cli": "^1.4.21",
    "@lowcodeunit-devkit/ng-lcu": "^1.12.71",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.6.4",
    "codelyzer": "^6.0.0",
    "concat-glob-cli": "^0.1.0",
    "domelementtype": "^2.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^11.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tsickle": "0.39.1",
    "tslib": "^2.1.0",
    "tslint": "~6.1.3",
    "typescript": "4.1.4"
  }
}
