{
  "name": "@lowcodeunit/lazy-element",
  "version": "0.0.150",
  "scripts": {
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lazy-element",
    "deploy:common": "npm version patch --prefix projects/common && ng build common && npm publish ./dist/common --access public",
    "deploy:lazy-element": "npm version patch --prefix projects/lazy-element && npm run build:lazy-element && npm publish ./dist/lazy-element --access public",
    "demo": "npm run build:common && npm run start",
    "build": "npm run build:common && npm run build:lazy-element",
    "build:common": "ng build common",
    "build:lazy-element": "ng build lazy-element --prod --single-bundle && npm run pack",
    "pack": "npm run pack:lazy-element",
    "pack:lazy-element": "mkdirp dist/wc/lazy-element && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:join": "concat-glob-cli -f \"dist/wc/lazy-element/lazy-element.*.js\" -o dist/wc/lazy-element/lazy-element.js",
    "pack:main": "concat-glob-cli -f \"dist/lazy-element/main.*.js\" -o dist/wc/lazy-element/lazy-element.1.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lazy-element/scripts.*.js\" -o dist/wc/lazy-element/lazy-element.0.js",
    "ng": "ng",
    "start": "ng serve demo --port=4282 --aot=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node projects/lazy-element/copy-wc-polyfill.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/elements": "~13.1.1",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@aspnet/signalr": "^1.1.4",
    "@lcu/common": "1.84.755-angular-13",
    "@webcomponents/custom-elements": "^1.5.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "copy": "0.3.2",
    "core-js": "^3.20.0",
    "document-register-element": "^1.14.10",
    "ngx-build-plus": "^13.0.1",
    "rxjs": "~7.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "~13.1.1",
    "@lcu/cli": "^1.7.97",
    "@lowcodeunit-devkit/ng-lcu": "^1.28.161",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.2",
    "concat-glob-cli": "^0.1.0",
    "domelementtype": "^2.2.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^13.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "tslib": "^2.3.1",
    "tslint": "~6.1.3",
    "typescript": "~4.5.4"
  }
}
